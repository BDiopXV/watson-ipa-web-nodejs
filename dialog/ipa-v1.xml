<?xml version="1.0" encoding="UTF-8"?>
<dialog xsi:noNamespaceSchemaLocation="WatsonDialogDocument_1.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <flow>
        <folder label="Main">
            <output>
                <prompt>
                    <item>Hello, how can I help you?</item>
                </prompt>
                <goto ref="getUserInput_2442994"/>
            </output>
            <output ref="getUserInput_2442993">
                <prompt>
                    <item>How else can I help you today?</item>
                </prompt>
                <getUserInput id="getUserInput_2442994">
                    <search ref="folder_2442998"/>
                    <default>
                        <output>
                            <prompt selectionType="RANDOM">
                                <item>I'm sorry I did not understand your question, please try again.</item>
                            </prompt>
                        </output>
                    </default>
                </getUserInput>
            </output>
        </folder>
        <folder label="Library">
            <folder label="Live Content" id="folder_2442998">
                <input>
                    <grammar>
                        <item>$ meeting</item>
                    </grammar>
                    <output id="output_2443150">
                        <prompt>
                            <item>Who should I invite to your meeting?</item>
                        </prompt>
                        <getUserInput>
                            <input>
                                <grammar>
                                    <item>$ (ContactFriendsList)={MeetingAttendee1}</item>
                                </grammar>
                                <action varName="MeetingAttendee1" operator="SET_TO">{MeetingAttendee1.value:main}</action>
                                <output>
                                    <prompt>
                                        <item>I'll invite {MeetingAttendee1}.</item>
                                    </prompt>
                                    <goto ref="output_2443160"/>
                                </output>
                            </input>
                            <input>
                                <grammar>
                                    <item>$ (ContactFamilyList)={MeetingAttendee1}</item>
                                </grammar>
                                <action varName="MeetingAttendee1" operator="SET_TO">{MeetingAttendee1.value:main}</action>
                                <output>
                                    <prompt>
                                        <item>I'll invite your {MeetingAttendee1}.</item>
                                    </prompt>
                                    <goto ref="output_2443160"/>
                                </output>
                            </input>
                            <output>
                                <prompt>
                                    <item>I can't find that person in your contacts.</item>
                                </prompt>
                                <goto ref="output_2443150"/>
                            </output>
                        </getUserInput>
                    </output>
                </input>
            </folder>
            <folder label="Meeting Time">
                <output id="output_2443160">
                    <prompt>
                        <item>What time is your meeting?</item>
                    </prompt>
                    <getUserInput>
                        <input>
                            <grammar>
                                <item>$ (DATE_TIME_RANGE)={MeetingTime}</item>
                            </grammar>
                            <action varName="MeetingTime" operator="SET_TO">{MeetingTime.value:FROM_TIME}</action>
                            <goto ref="output_2443170"/>
                        </input>
                        <output>
                            <prompt>
                                <item>I'll need a valid time to continue.</item>
                            </prompt>
                            <goto ref="output_2443160"/>
                        </output>
                    </getUserInput>
                </output>
            </folder>
            <folder label="Meeting Date">
                <output id="output_2443170">
                    <prompt>
                        <item>What is the date for your meeting?</item>
                    </prompt>
                    <getUserInput>
                        <input>
                            <grammar>
                                <item>$ (DATE_TIME_RANGE)={MeetingDate}</item>
                            </grammar>
                            <action varName="MeetingDate" operator="SET_TO">{MeetingDate.value:FROM_DATE}</action>
                            <output>
                                <prompt>
                                    <item>
                                        I've invited {MeetingAttendee1} to your meeting at {MeetingTime} on {MeetingDate}.
                                    </item>
                                </prompt>
                                <goto ref="getUserInput_2442993"/>
                            </output>
                        </input>
                        <output>
                            <prompt>
                                <item>I'll need a valid date to continue.</item>
                            </prompt>
                            <goto ref="output_2443170"/>
                        </output>
                    </getUserInput>
                </output>
            </folder>
            <folder label="Storage"/>
        </folder>
        <folder label="Global"/>
        <folder label="Concepts">
            <concept>
                <grammar>
                    <item>VPA</item>
                    <item>Virtual Agent</item>
                    <item>Virtual Personal Agent</item>
                </grammar>
            </concept>
        </folder>
    </flow>
    <entities>
        <entity name="ContactFriendsList">
            <value name="Anthony" value="Anthony"/>
            <value name="Kelly" value="Kelly"/>
            <value name="Ryan" value="Ryan"/>
            <value name="Tina" value="Tina"/>
        </entity>
        <entity name="ContactFamilyList">
            <value name="mother" value="mother"/>
            <value name="father" value="father"/>
            <value name="wife" value="wife"/>
            <value name="husband" value="husband"/>
            <value name="brother" value="brother"/>
            <value name="sister" value="sister"/>
        </entity>
    </entities>
    <constants>
        <var_folder name="Home">
            <var_folder name="General" type="CONST">
                <var name="VA" type="TAG">Virtual Agent</var>
            </var_folder>
        </var_folder>
    </constants>
    <variables>
        <var_folder name="Home">
            <var name="MeetingAttendee1" type="TEXT" description="First person to invite to the meeting"/>
            <var name="MeetingAttendee2" type="TEXT" description="Second person to invite to the meeting"/>
            <var name="MeetingDate" type="TEXT" description="Date for the meeting"/>
            <var name="MeetingTime" type="TEXT" description="Time for the meeting"/>
        </var_folder>
    </variables>
